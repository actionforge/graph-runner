{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üü¢ Run action graph",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": [],
      "buildFlags": "-tags=github_impl",
      "hideSystemGoroutines": true,
      "env": {
        "LOAD_ENV_FILE": "true",
        "GITHUB_ACTIONS": "true"
      }
    },
    {
      "name": "üì¶ Freeze action graph",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": [
        "freeze",
        "system_tests/test_freeze.yml",
        "--output",
        "~/frozen"
      ],
      "buildFlags": "",
      "hideSystemGoroutines": true,
      "console": "integratedTerminal"
    },
    {
      "name": "üß™ Run Current Test File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${file}",
      "buildFlags": "-tags=github_impl,system_tests",
      "env": {},
      "args": [],
      "showLog": true
    },
    {
      "name": "‚öôÔ∏è Generate node stubs",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags=github_impl,generate_node_stubs",
      "args": [
        "generate"
      ],
      "env": {
        "LOAD_ENV_FILE": "true",
      },
      "hideSystemGoroutines": true,
    },
    {
      "name": "‚ÜóÔ∏è Update Registry",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags=github_impl,update_registry",
      "args": [
        "update"
      ],
      "env": {},
      "hideSystemGoroutines": true,
    },
  ]
}